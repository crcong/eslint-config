{
  "name": "@crcong/eslint-config-monorepo",
  "version": "0.3.0",
  "description": "eslint config",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "crcong",
  "scripts": {
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "keywords": [
    "eslint"
  ],
  "license": "MIT",
  "dependencies": {
    "bumpp": "^7.1.1",
    "eslint": "^8.12.0",
    "typescript": "^4.6.3"
  }
}
